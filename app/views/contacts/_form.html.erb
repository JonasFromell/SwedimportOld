<% form_for @contact, :url => { :action => "create" }, :html => { :method => :post, :id => "contactForm" } do |f| %>
	<p class="oneHalf">
		<%= f.label "Namn" %>
		<%= f.text_field :sender_name %>
	</p>
	
	<p class="oneHalf">
		<%= f.label "E-mail" %>
		<%= f.text_field :email %>
	</p>
	
	<p>
		<%= f.label "Meddelande" %>
	</p>
	<p>
		<%= f.text_area :content %>
	</p>
	
	<p>
		<%= f.submit "Skicka" %>
	</p>
<% end %>